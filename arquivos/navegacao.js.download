angular.module('model')
    .config(['$routeProvider', function ($routeProvider) {

        $routeProvider
            .when('/', {
                templateUrl: 'views/orcamento-controle-pragas.html',
                controller: 'orcamento-controle-pragas'
            })
            .when('/orcamento-controle-pragas', {
                templateUrl: 'views/orcamento-controle-pragas.html',
                controller: 'orcamento-controle-pragas'
            })
            .when('/perfil-empresa', {
                templateUrl: 'views/perfil-empresa.html',
                controller: 'perfil-empresa'
            })
            .when('/orcamentos', {
