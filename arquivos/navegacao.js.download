                controller: 'orcamento-controle-pragas'
            })
            .when('/controle-pragas', {
                templateUrl: 'views/orcamento-controle-pragas.html',
                controller: 'orcamento-controle-pragas'
            })
            .when('/historico-ordem-servico', {
                templateUrl: 'views/historico-ordem-servico.html',
                controller: 'historico-ordem-servico'
